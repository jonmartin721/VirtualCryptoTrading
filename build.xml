    <copy file="${temp.jar.path.CryptoProject.jar}" tofile="${artifact.temp.output.cryptoproject:jar}/CryptoProject.jar"/>
  </target>
  
  <target name="build.all.artifacts" depends="artifact.cryptoproject:jar" description="Build all artifacts">
    <mkdir dir="${artifact.output.cryptoproject:jar}"/>
    <copy todir="${artifact.output.cryptoproject:jar}">
      <fileset dir="${artifact.temp.output.cryptoproject:jar}"/>
    </copy>
    
    <!-- Delete temporary files -->
    <delete dir="${artifacts.temp.dir}"/>
