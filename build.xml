    <mkdir dir="${artifact.temp.output.cryptoproject:jar}"/>
    <jar destfile="${temp.jar.path.CryptoProject.jar}" duplicate="preserve" filesetmanifest="mergewithoutmain">
      <zipfileset dir="${cryptoproject.output.dir}"/>
      <zipfileset src="${path.variable.maven_repository}/org/knowm/xchange/xchange-core/4.2.0/xchange-core-4.2.0.jar"/>
