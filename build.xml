      <zipfileset src="${path.variable.maven_repository}/com/google/code/gson/gson/2.8.2/gson-2.8.2.jar"/>
      <zipfileset src="${path.variable.maven_repository}/com/squareup/retrofit2/retrofit/2.4.0/retrofit-2.4.0.jar"/>
