    <fileset dir="${javac2.home}">
      <include name="javac2.jar"/>
      <include name="jdom.jar"/>
      <include name="asm-all*.jar"/>
      <include name="jgoodies-forms.jar"/>
    </fileset>
  </path>
  <target name="register.custom.compilers">
    <taskdef name="javac2" classname="com.intellij.ant.Javac2" classpathref="javac2.classpath"/>
    <taskdef name="instrumentIdeaExtensions" classname="com.intellij.ant.InstrumentIdeaExtensions" classpathref="javac2.classpath"/>
  </target>
  
  <!-- Modules -->
  
  
  <!-- Module CryptoProject -->
  
  <dirname property="module.cryptoproject.basedir" file="${ant.file}"/>
  
  
  <property name="module.jdk.home.cryptoproject" value="${project.jdk.home}"/>
  <property name="module.jdk.bin.cryptoproject" value="${project.jdk.bin}"/>
  <property name="module.jdk.classpath.cryptoproject" value="${project.jdk.classpath}"/>
  
  <property name="compiler.args.cryptoproject" value="-encoding UTF-8 -source 9 -target 9 ${compiler.args}"/>
  
  <property name="cryptoproject.output.dir" value="${module.cryptoproject.basedir}/CryptoProject/target/classes"/>
  <property name="cryptoproject.testoutput.dir" value="${module.cryptoproject.basedir}/CryptoProject/target/test-classes"/>
  
  <path id="cryptoproject.module.bootclasspath">
    <!-- Paths to be included in compilation bootclasspath -->
  </path>
  
  <path id="cryptoproject.module.production.classpath">
    <path refid="${module.jdk.classpath.cryptoproject}"/>
  </path>
  
  <path id="cryptoproject.runtime.production.module.classpath">
    <pathelement location="${cryptoproject.output.dir}"/>
  </path>
  
  <path id="cryptoproject.module.classpath">
    <path refid="${module.jdk.classpath.cryptoproject}"/>
    <pathelement location="${cryptoproject.output.dir}"/>
  </path>
  
  <path id="cryptoproject.runtime.module.classpath">
    <pathelement location="${cryptoproject.testoutput.dir}"/>
    <pathelement location="${cryptoproject.output.dir}"/>
  </path>
  
  
  <patternset id="excluded.from.module.cryptoproject">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.cryptoproject">
    <patternset refid="excluded.from.module.cryptoproject"/>
  </patternset>
  
  <path id="cryptoproject.module.sourcepath">
    <dirset dir="${module.cryptoproject.basedir}/CryptoProject">
      <include name="src"/>
    </dirset>
  </path>
  
  
  <target name="compile.module.cryptoproject" depends="compile.module.cryptoproject.production,compile.module.cryptoproject.tests" description="Compile module CryptoProject"/>
  
  <target name="compile.module.cryptoproject.production" depends="register.custom.compilers" description="Compile module CryptoProject; production classes">
    <mkdir dir="${cryptoproject.output.dir}"/>
    <javac2 destdir="${cryptoproject.output.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.cryptoproject}/javac">
      <compilerarg line="${compiler.args.cryptoproject}"/>
      <bootclasspath refid="cryptoproject.module.bootclasspath"/>
      <classpath refid="cryptoproject.module.production.classpath"/>
      <src refid="cryptoproject.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.cryptoproject"/>
    </javac2>
    
    <copy todir="${cryptoproject.output.dir}">
      <fileset dir="${module.cryptoproject.basedir}/CryptoProject/src">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="compile.module.cryptoproject.tests" depends="register.custom.compilers,compile.module.cryptoproject.production" description="compile module CryptoProject; test classes" unless="skip.tests"/>
  
  <target name="clean.module.cryptoproject" description="cleanup module">
    <delete dir="${cryptoproject.output.dir}"/>
    <delete dir="${cryptoproject.testoutput.dir}"/>
  </target>
  
  <target name="init" description="Build initialization">
    <!-- Perform any build initialization in this target -->
  </target>
  
  <target name="clean" depends="clean.module.cryptoproject" description="cleanup all"/>
  
  <target name="build.modules" depends="init, clean, compile.module.cryptoproject" description="build all modules"/>
  
  <target name="init.artifacts">
    <property name="artifacts.temp.dir" value="${basedir}/__artifacts_temp"/>
    <property name="artifact.output.cryptoproject:jar" value="${basedir}/out/artifacts/CryptoProject_jar"/>
    <mkdir dir="${artifacts.temp.dir}"/>
    <property name="temp.jar.path.CryptoProject.jar" value="${artifacts.temp.dir}/CryptoProject.jar"/>
  </target>
  
